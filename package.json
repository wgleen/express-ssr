{
  "name": "express-ssr",
  "version": "1.0.0",
  "description": "'Express middleware for SSR'",
  "main": "lib/index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib",
    "prepublishOnly": "npm run build",
    "postpublish": "rm -R lib",
    "test": "jest"
  },
  "files": [
    "lib",
    "src"
  ],
  "npmName": "express-ssr",
  "npmFileMap": [
    {
      "basePath": "/lib/",
      "files": [
        "*.js"
      ]
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wgleen/express-ssr.git"
  },
  "keywords": [
    "express",
    "nodejs",
    "spa"
  ],
  "author": "Wallace Gleen <wallace.gleen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wgleen/express-ssr/issues"
  },
  "homepage": "https://github.com/wgleen/express-ssr#readme",
  "dependencies": {
    "puppeteer": "^1.13.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-async-to-generator": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4"
  }
