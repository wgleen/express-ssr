{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-transform-async-to-generator",
    [
      "@babel/plugin-transform-runtime", 
      { "regenerator": true }
    ]
  ],
  "env": {
    "commonjs": {
      "presets": [
        ["@babel/preset-env", {
          "useBuiltIns": false
        }]
      ],
      "plugins": [
        "add-module-exports"
      ]
    },
    "es": {
      "presets": [
        ["@babel/preset-env", {
          "useBuiltIns": false,
          "modules": false
        }]
      ]
    }
  },
  "sourceMaps": true,
  "retainLines": true
}